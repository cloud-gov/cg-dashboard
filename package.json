    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "classnames": "^2.1.5",
    "cloudgov-style": "^1.3.0",
    "codecov.io": "^0.1.5",
    "css-loader": "^0.23.0",
    "dedent": "^0.6.0",
